{
    "name": "claroline/migration-bundle",
    "description": "Claroline migration bundle",
    "keywords": ["Claroline", "Migration"],
    "homepage": "http://www.claroline.net",
    "type": "bundle",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Claroline development team",
            "email": "dev@claroline.net",
            "homepage": "http://github.com/claroline"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url" : "https://github.com/djerrah/migrations.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "*",
        "symfony/console": "@stable",
        "symfony/twig-bridge": "@stable",
        "symfony/twig-bundle": "@stable",
        "doctrine/orm": "@stable",
        "doctrine/migrations": "dev-develop",
        "jdorn/sql-formatter": "@stable",
        "psr/log": "@stable",
        "claroline/kernel-bundle": "~6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "mikey179/vfsStream": "~1.6"
    },
    "autoload": {
        "psr-4": { "Claroline\\MigrationBundle\\": "" }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
